a=int(input('Enter a value:'))
b=int(input('Enter b value:'))
c=int(input('Enter c value:'))
d=int(input('Enter d value:'))
if a>b:
    if a>c:
        if a>d:
            print('a is big')
if b>a:
    if b>c:
        if b>d:
            print('b is big')
if c>a:
    if c>b:
        if c>d:
            print('c is big')
if d>a:
    if d>b:
        if d>c:
            print('d is big')
else:
    print('All number are same')
