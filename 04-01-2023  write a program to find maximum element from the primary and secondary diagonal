n = int(input())
m = []
z = []
c = 0
for i in range(0,n):
    l = []
    for j in range(0,n):
        x = int(input())
        l.append(x)
    m.append(l)
for i in range(0,n):
    for j in range(0,n):
        if i == j:
            z.append(m[i][j])
        if i+j == n-1:
            z.append(m[i][j])
print(z)
print(max(z))
